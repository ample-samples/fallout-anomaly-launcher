{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.web.json"
    }
  ],
  "rules": {
    "react/prop-types": "off"
  },
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ],
      "@main/*": [
        "src/main/*"
      ],
      "@preload/*": [
        "src/preload/*"
      ],
      "@assets/*": [
        "src/renderer/src/assets"
      ],
      "@/*": [
        "src/renderer/src/*"
      ]
    }
  }
}
